{
  "name": "angularjs-seed",
  "version": "1.0.0",
  "description": "Seed para criação de projetos com AngularJS, usando ES2015",
  "scripts": {
    "babel": "babel-node --presets es2015",
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "npm run babel -- ./node_modules/karma/bin/karma start",
    "test-watch": "npm run babel -- ./node_modules/karma/bin/karma start --auto-watch --no-single-run",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
